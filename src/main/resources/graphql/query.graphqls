type Query {
    getAllUsers(size: Int! @Min(value: 1), page: Int! @Min): [User]
    getAllPosts(size: Int! @Min(value: 1), page: Int! @Min): [Post]
    findPostsByAuthor(authorId: String! @NotBlank, cursor: String! @NotBlank, page: Int! @Min): [Post]
    findPostsByLiker(likerId: String! @NotBlank, cursor: String! @NotBlank, page: Int! @Min): [Post]
    findAuthorTopPosts(authorId: String! @NotBlank, count: Int! @Min(value: 1)): [Post]
}