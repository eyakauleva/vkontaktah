type Query {
    getAllUsers(size: Int! @Min(value: 1), page: Int! @Min): [User]
    getAllPosts(size: Int! @Min(value: 1), page: Int! @Min): [Post]
    findPostsByAuthor(authorId: String! @NotBlank, size: Int! @Min(value: 1), page: Int! @Min): [Post]
    findPostsByLiker(likerId: String! @NotBlank, size: Int! @Min(value: 1), page: Int! @Min): [Post]
}